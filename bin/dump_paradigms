#!/bin/sh

curl -H 'Content-Type: application/json' \
  -H 'Accept: application/json' \
  -X POST http://localhost:3000/signin \
  -d '{"login" : "alpha", "password" : "111"}' \
  -c cookie

curl -H 'Content-Type: application/json' \
  -H 'Accept: application/json' \
  -X GET http://localhost:3000/paradigms.txt \
  -b cookie
